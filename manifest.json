{
	"name": "Image Replacer",
	"description": "Replace all images with whatever you want.",
	"author": "MysteryPancake",
	"manifest_version": 2,
	"version": "1.0",
	"browser_action": {
		"default_title": "Image Replacer",
		"default_popup": "index.html"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"run_at": "document_end",
			"js": ["scripts/Main.js"],
			"all_frames": true
		}
	],
	"homepage_url": "https://github.com/MysteryPancake/Image-Replacer",
	"permissions": ["storage"]
}